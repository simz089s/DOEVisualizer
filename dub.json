	"dependencies": {
        "excel-d": "*",
        "mir": "*",
        "xlsxd": "*",
	},
	"description": "Metal Analysis Report",
	"license": "MIT",
    "name": "metalanalysisreport",
    "sourceFiles": [ "src/app.d", "src/funcs.d" ],
    "targetType": "dynamicLibrary",
    "dflags": [ "-dip25", "-dip1000" ],
    "configurations": [
        {
            "name": "xll",
            "preBuildCommands": [ "dub run -c def --nodeps -q -- metalanalysisreport.def" ],
            "sourceFiles": [ "metalanalysisreport.def" ],
            "libs": [ "xlcall32" ],
            "postBuildCommands": [ "copy /y metalanalysisreport.dll metalanalysisreport.xll" ]
        },
        {
            "name": "justxll",
            "sourceFiles": [ "metalanalysisreport.def" ],
            "libs": [ "xlcall32" ],
            "postBuildCommands": [ "copy /y metalanalysisreport.dll metalanalysisreport.xll" ]
        },
        {
            "name": "def",
            "targetType": "executable",
            "targetName": "write_def",
            "versions": [ "exceldDef" ]
        }
    ]
}
